    "eslint-config-rational": "^6.0.6",
    "react": "^18.3.1",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-live-preview": "^0.2.1",
    "vite-plugin-config-lib": "^0.2.0",
    "vite-plugin-data": "^0.2.5",
    "vite-plugin-dts": "^3.9.1"
